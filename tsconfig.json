{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "allowJs": true,
    "outDir": "./dist/",
    "noEmit": true,
    // "strict": true,
    // "noImplicitAny": true,
    
    /* Additional Checks */
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,

    /* Module Resolution Options */
    "moduleResolution": "node",
    "esModuleInterop": true,

    "baseUrl": ".",
    "paths": {
      "@/*": [
        "alias/*"
      ],
    }
  }
}
