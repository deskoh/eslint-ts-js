{
  "name": "eslint-ts-js",
  "version": "1.0.0",
  "description": "ESLint configuration for mixed-type project with TypeScript and JavaScript files.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c ts.eslintrc --ext=js,ts .",
    "lint:babel": "eslint -c babel.eslintrc --ext=js,ts .",
    "lint:ts-babel": "eslint -c ts-babel.eslintrc --ext=js,ts ."
  },
  "keywords": [
    "eslint"
  ],
  "author": "deskoh",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2"
  },
  "dependencies": {
    "typescript": "^3.5.3"
  }
}
